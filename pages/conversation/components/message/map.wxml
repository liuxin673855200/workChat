<view bindtap='openMap' class='mapcontent'>
      <view class='title'> {{message.content.content.name}}  </view>
    
      <map
        bindtap='openMap'
        class="map"
        longitude="{{message.content.content.longitude}}"
        latitude="{{message.content.content.latitude}}"
        enable-zoom= "{{isFalse}}"
        enable-scroll = "{{isFalse}}"
    ></map>
    
</view>
