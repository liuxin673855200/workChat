/**index.wxss**/
.contain{
  width: 100%;
  height: 100vh;
  padding:0rpx;
  margin:0 auto;
  background: #FCFCFC;
  text-align: center;
   background-image: url("https://s.chedianzhang.com/res/backstage/0119Revision/images/wechat/titlefoot.png");
background-size:550rpx 400rpx;
background-repeat:no-repeat;
background-position: 0% 100%;
position: fixed;
padding-top: 100rpx;
/* bottom: 0rpx; */
}
.carOpenimg{
  background: url("https://s.chedianzhang.com/res/backstage/0119Revision/images/wechat/a6ea01a83237d28ffb46a9f4df4d781.png");
background-repeat:no-repeat;
background-position: 50% 90%;
height: 115rpx;width: 115rpx;
background-size: cover;
margin: 0 auto;
margin-bottom: 40rpx;
}
.factoryState::before{
  content: "";
display: block;
width: 143rpx;
height: 1rpx;
background: #858488;
position:absolute;
top:8px;
left: 5%;
}
.factoryState{
  font-size: 12px;
color: #858488;
letter-spacing: 2px;
position: relative;
margin-top: 30rpx;
}
.factoryState::after{
  content: "";
display: block;
width: 143rpx;
height: 1rpx;
background: #858488;
position:absolute;
top:8px;
right: 5%;
}
.carOpen{
  font-family: PingFangSC-Semibold;
  font-size: 22px;
  font-weight: bold;
color: #43496A;
letter-spacing: 8.55px;
}
.authorCar{
  font-size: 12px;
color: #858488;
letter-spacing: 2px;
margin-top:165rpx;
width:80%;
margin:165rpx auto 40rpx;
}
.carOpenbtn{
  background: #2CA347;
border-radius: 46.15px;
width:80%;
margin: 0 auto 30rpx;
color:#fff;
height: 100rpx;
font-size: 17px;
line-height: 100rpx;
}
.carOpenYes{
  font-size: 12px;
color: #858488;
margin-top: 40rpx;
}
.yzmerror{
  font-family: PingFangSC-Regular;
font-size: 14px;
text-align: left;
padding-left: 60rpx;
color: #EC001D;
letter-spacing: 1.31px;
}
.maskLayer {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.2;
  overflow: hidden;
  z-index: 1000;
  color: #fff;
}
button[disabled]{
background: #dddddd;
box-shadow: 0 0 17px 0 gray;
}